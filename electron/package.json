    "electron-window-state": "~5.0.3",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.41",
    "typeorm-test-transactions": "^3.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
